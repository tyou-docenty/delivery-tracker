{
  "version": 2,
  "build": {
    "src": "/packages/server/dist/index.js",
    "env": {
      "NODE_ENV": "production"
    }
  },
  "buildCommand": "pnpm --filter @delivery-tracker/server build-with-deps",
  "routes": [
    {
      "src": "/packages/server",
      "dest": "/packages/server/dist/index.js"
    }
  ],
  "outputDirectory": "/packages/server"
}
