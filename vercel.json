{
  "version": 2,
  "rootDirectory": "packages/server",
  "buildCommand": "pnpm build-with-deps",
  "outputDirectory": "dist",
  "installCommand": "pnpm install",
  "devCommand": "pnpm start",
  "functions": {
    "dist/index.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
